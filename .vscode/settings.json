    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.vscode": false,
        "**/*.VC.db": true,
        "bin/": true,
        "CMakeFiles": true,
        "cmake_install.cmake": true,
